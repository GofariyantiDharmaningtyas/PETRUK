#include<iostream>
#include <cstring>
using namespace std;
int main(){
    char A[] = {'N','A','U','R','A','H','O','K','E','M'};
    char B[] = {'A','I','P','U','Y','O','L','O','A','E'};
    char C[] = {'A','L','N','O','T','H','I','S','G','I'};
    char D[] = {'Y','A','O','G','A','U','A','Y','O','K'};
    char E[] = {'A','L','O','H','G','O','H','Y','O','A'};
    char F[] = {'B','W','I','N','T','E','R','O','O','R'};
    char G[] = {'N','A','B','L','O','O','D','B','T','T'};
    char H[] = {'Y','O','L','O','O','L','O','Y','G','A'};
    char I[] = {'A','D','A','I','T','U','S','A','J','A'};
    char J[] = {'Q','W','N','O','N','H','S','I','N','G'};
    char* K[] = {A,B,C,D,E,F,G,H,I,J};

for(int i = 0; i<10; i++){
        for(int j=0; j<10; j++) 
        cout<<*(*(K+i)+j)<<"  ";
		cout<<endl;
    }
     int jml,hsl[jml], ha[jml], p[jml] , dia;
    cout<<endl;
    cout<<"Jumlah Kata yang akan dicari : ";cin>>jml;
    char kata[jml][10];
}
cin.ignore();
    for(int i=1; i<jml; i++){
        cout<<i<<" ";
	cin.getline(kata[i], 10);
        p[i]=strlen(kata[i]);   
    }
for(int n=0; n<jml; n++) {
        hsl[n]=0;
        int pk = p[n], y=0;
        bool ada[pk], result;
        for(int s=0;s<10;s++){
            for(int i=0;i<=(10-pk);i++) {
                for(int j=i; j<=i+(pk-1);j++) {
                    if(*(*(K+s)+j) == kata[n][y]) ada[y] = 1;
                    else ada[y] = 0;y++;
                }
		for(int l=0; l<pk;l++){
                    if(ada[l]==1) {
                        result = 1;
                        hsl[n]+=result;
                    }
                else {
                    result = 0;
                    break;
                    }
                    if(hsl[n]==pk) { 
                        ha[n]=1; 
                        goto next; 
                    }
		   }
                hsl[n]=0;
                y=0;
            }
	   }
for(int g=0;g<10;g++){
            for(int i=0;i<=(10-pk);i++) {
                for(int j=i;j<=i+(pk-1);j++) {
                    if(*(*(K+j)+g) == kata[n][y]) ada[y] = 1;
                    else ada[y] = 0;y++;
                }
                for(int t=0;t<pk;t++){
                    if(ada[t]==1){
                        result = 1;
                        hsl[n]+=result;
                    }
                else{
                result = 0;break;
                    }
                    if(hsl[n]==pk) { 
                        ha[n]=1; 
                        goto next; 
                    }
                }
                hsl[n]=0;
                y=0;
            }
        }
for(int hn=0;hn<10;hn++){
            for(int i=0; i<=(10-pk); i++) {
            int dia=-1;
                for(int j=i; j<=i+(pk-1); j++) {
                    ++dia;
                    if(*(*(K+j)+hn+dia) == kata[n][y]) ada[y] = 1;
                    else ada[y]=0;y++;
                }
                for(int t=0; t<pk; t++){
                    if(ada[t]==1){
                        result = 1;
                        hsl[n]+=result;
                    }
                    else{
                        result = 0;
                        break;
                    }
                    if(hsl[n]==pk) { 
                        ha[n]=1; 
                        goto next; 
                    }
                }
                hsl[n]=0;
                y=0;
            }
        }
for(int hr=14;hr>=0;hr--){
            for(int i=0; i<=(10-pk); i++) {
            int dia=-1;
                for(int j=i; j<=i+(pk-1); j++) {
                    ++dia;
                    if(*(*(K+j)+hr-dia) == kata[n][y]) ada[y] = 1;
                    else ada[y] = 0;y++;
                }
                for(int t=0; t<pk; t++) {
                    if(ada[t]==1) {
                        result = 1;
                        hsl[n]+=result;
                    }
                else {
                        result = 0;
                        break;
                    }
                if(hsl[n]==pk) { 
                        ha[n]=1; 
                        goto next; 
                    }
                }
                hsl[n]=0;
                y=0;
            }
        }
        next:
        y=0;
    }
     for(int i=0;i<jml;i++){
        if(ha[i]==1){
        cout<<i+1<<" ADA"<<endl;
    }else {
	cout<<i+1<<" TIDAK ADA"<<endl;
    }
    return 0;
}
}
